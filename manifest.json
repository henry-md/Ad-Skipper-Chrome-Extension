{ 
  "name": "Ad Skipper", 
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Adds keyboard commands to speed up all video players in the window. Skip ads, or just speed things up.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["default.css"],
      "js": ["default.js"],
      "all_frames": true
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}